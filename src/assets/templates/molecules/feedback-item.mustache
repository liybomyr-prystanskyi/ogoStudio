{{#mol-feedback-item}}
{{#feedback-items}}
<div class="row justify-content-between align-items-center">
  <div class="col-12 col-lg-7" data-aos="fade-right">
    <div class="feedback__main__text">
      <div class="feedback__main__text__object">
        <div class="feedback__main__text__object__name">

          {{#image}}
          <img src="{{{image}}}" alt="{{{alt}}}">
          {{/image}}

          <div class="feedback__main__text__object__name__position">

            {{#name}}
            <h3>{{{name}}}</h3>
            {{/name}}

            {{#position}}
            <p>{{{position}}}</p>
            {{/position}}
          </div>

        </div>

        {{#description}}
        <p>
         {{{description}}}
        </p>
        {{/description}}

      </div>
    </div>
  </div>

  <div class="col-12 col-lg-4" data-aos="fade-left">
    <div class="feedback__main__percent">

      {{#percent}}
      <h3>{{{percent}}}</h3>
      {{/percent}}

      {{#percent-text}}
      <p>{{{percent-text}}}</p>
      {{/percent-text}}
      
    </div>
  </div>
</div>
{{/feedback-items}}
{{/mol-feedback-item}}