{{#mol-feedback-item}}
{{#feedback-items}}

<div class="d-flex justify-content-between align-items-end {{{margin-bottom-124}}} ">
  <div class="feedback__main__text">
    <div class="feedback__main__text__object">
      <div class="feedback__main__text__object__name">
        {{#image}}
        <img src="{{{image}}}" alt="{{{alt}}}">
        {{/image}}
        <div class="feedback__main__text__object__name__position">
          {{#name}}
          <h3>{{{name}}}</h3>
          {{/name}}
          {{#position}}
          <p>{{{position}}}</p>
          {{/position}}
        </div>
      </div>
<div class="row justify-content-around">
  <div class="col-12 col-lg-6 col-md-6">
    {{#description}}
    <p>
     {{{description}}}
    </p>
    {{/description}}
  </div>

  <div class="col-12 col-lg-4 col-md-6">
    <div class="feedback__main__percent">
    {{#percent}}
    <h3>{{{percent}}}</h3>
    {{/percent}}

    {{#percent-text}}
    <p>{{{percent-text}}}</p>
    {{/percent-text}}
  </div>
</div>
</div>

    </div>
  </div>
</div>
{{/feedback-items}}
{{/mol-feedback-item}}